body{
	background-color:#CAD1DD;
}

#cabecalho{
	font-weight:bold;
	font-family:arial;
	font-size:65px;
	padding:50px;
	background:#274558;
	text-align:center;
	color:#fff;
}

#conteudo-1{
	text-align:justify;
	float:left;
	width:210px;
	font-family:arial;
	padding:30px;
	padding-bottom:50px;
	margin-left:15px;
	margin-top:1px;
	background:#006B9B;
	color:#FFFFFF;
}

#conteudo-3{
	text-align:center;
	float:right;
	width:210px;
	font-family:arial;
	padding:30px;
	padding-bottom:50px;
	margin-right:15px;
	margin-top:1px;
	background:#006B9B;
	color:#FFFFFF;
	margin-bottom:15px;
	font-size:21px;
}

#conteudo-3 a{
	text-decoration:none;
	color:#FFFFFF;
}

#conteudo-2{
	text-align:justify;
	float:center;
	width:650px;
	margin-top:25px;
	margin-left:auto;
	margin-right:auto;
	padding:20px;
	background:#FFFFFF;
}

#conteudo-2 a{
	font-size:26px;
	text-align:justify;
	color:#000000;
	font-family:arial;
	text-decoration:none;
	font-weight:bold;
}
	
#conteudo-4{
	text-align:justify;
	float:center;
	width:650px;
	font-family:arial;
	margin-left:auto;
	margin-right:auto;
	padding:20px;
	padding-top:6px;
	padding-bottom:5px;
	background:#FFFFFF;
	color:#8c8787;
	margin-bottom:25px;
	font-size:20px;
}

#rodape{
	clear:both;
	padding:15px;
	font-family:arial;
	background:#274558;
	text-align:center;
	color:#FFFFFF;
	margin-top:50px;
}